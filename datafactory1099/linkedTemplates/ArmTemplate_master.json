{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "datafactory1099"
		},
		"AzureDataLakeStorage1_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorage1'"
		},
		"azureSynapseworkspace_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'azureSynapseworkspace'"
		},
		"AzureDataLakeStorage1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://datalake1099.dfs.core.windows.net/"
		},
		"azureSynapseworkspace_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "dataworkspace1099.sql.azuresynapse.net"
		},
		"azureSynapseworkspace_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "datapool"
		},
		"azureSynapseworkspace_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "sqladminuser"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "datafactory1099_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureDataLakeStorage1_accountKey": {
						"value": "[parameters('AzureDataLakeStorage1_accountKey')]"
					},
					"azureSynapseworkspace_password": {
						"value": "[parameters('azureSynapseworkspace_password')]"
					},
					"AzureDataLakeStorage1_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorage1_properties_typeProperties_url')]"
					},
					"azureSynapseworkspace_properties_typeProperties_server": {
						"value": "[parameters('azureSynapseworkspace_properties_typeProperties_server')]"
					},
					"azureSynapseworkspace_properties_typeProperties_database": {
						"value": "[parameters('azureSynapseworkspace_properties_typeProperties_database')]"
					},
					"azureSynapseworkspace_properties_typeProperties_userName": {
						"value": "[parameters('azureSynapseworkspace_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}